       name: 'Access source',\n                    type: 'pie',\n                    radius: '55%',\n                    center: ['50%', '60%'],\n                    color: ['#FF9F43', '#28C76F', '#EA5455', '#87ceeb', '#7367F0'],\n                    data: [\n                        { value: 335, name: 'Direct interview' },\n                        { value: 310, name: 'Email marketing' },\n                        { value: 234, name: 'Alliance advertising' },\n                        { value: 135, name: 'Video ad' },\n                        { value: 1548, name: 'Search engine' }\n                    ],\n                    itemStyle: {\n                        emphasis: {\n                            shadowBlur: 10,\n                            shadowOffsetX: 0,\n                            shadowColor: 'rgba(0, 0, 0, 0.5)'\n                        }\n                    }\n                }]\n            },\n        }\n    },\n    components: {\n        ECharts\n    },\n    mounted() {\n        let dataIndex = -1\n        let pie = this.$refs.pie\n        let dataLen = pie.options.series[0].data.length\n        setInterval(() => {\n            pie.dispatchAction({\n                type: 'downplay',\n                seriesIndex: 0,\n                dataIndex\n            })\n            dataIndex = (dataIndex + 1) % dataLen\n            pie.dispatchAction({\n                type: 'highlight',\n                seriesIndex: 0,\n                dataIndex\n            })\n            pie.dispatchAction({\n                type: 'showTip',\n                seriesIndex: 0,\n                dataIndex\n            })\n        }, 1000)\n    }\n}\n",{"version":3,"sources":["EchartsPieChart.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAoGA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;A